<div class='address_form'>
<%= form_for address, :remote => true do |f| %>
 	<%= f.label "Street Address" %>
  	<%= f.text_field :street_address %>
  	<%= f.label "City" %>
  	<%= f.text_field :city %>
  	<%= f.label "State" %>
  	<%= f.text_field :state %>
  	<%= f.label "Zip Code" %>
  	<%= f.text_field :zip %>
	<%= f.hidden_field :company_id, :value => 33 %>
	<br /><%= f.submit "Submit" %>
<% end %>
</div>